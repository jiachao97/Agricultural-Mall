<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.jc.dao.SupplyMapper">
	
	<!-- 查询供应商品 -->
	<select id="getSupplyList" parameterType="com.jc.pojo.Supply" resultType="com.jc.pojo.Supply">
		select * from supply
	</select>
	
	<!-- 查询供应商品总记录数 -->
	<select id="getSupplyListCount" parameterType="com.jc.pojo.Supply" resultType="java.lang.Integer">
		select count(*) from supply
	</select>
	
</mapper>